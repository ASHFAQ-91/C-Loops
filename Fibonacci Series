// Write a C++ program to print first N terms of Fibanacci series.
#include<iostream>
using namespace std;
int main()
{
    int a=0, b=1, n, sum=0;
    cout << "Enter number: ";
    cin >> n;
    cout << "0\t1\t";
    for(int i=2; i<n; i++)
    {
        sum = a + b;
        cout << sum << "\t";
        a = b;
        b = sum;
    }
}
